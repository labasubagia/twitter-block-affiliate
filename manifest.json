{
  "manifest_version": 2,
  "name": "X.com Block Affiliate",
  "version": "1.0",
  "description": "Block affiliate when visiting x.com",
  "permissions": ["tabs", "http://*/*", "https://*/*","activeTab"],
  "content_scripts": [
    {
      "matches": ["*://x.com/*"],
      "js": ["script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html",
    "default_title": "X.com Block Affiliate"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
